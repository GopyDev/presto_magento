    <script type="text/javascript">

        jQuery(document).ready(function ()
        {
            jQuery("#btnShowSimple").click(function (e)
            {
                ShowDialog(false);
                e.preventDefault();
            });
			jQuery("#btnClose").click(function (e)
            {
                HideDialog();
                e.preventDefault();
				 location.reload();
            });
			jQuery("#btnSubmit").click(function (e)
            {
                HideDialog();
                e.preventDefault();
				var href_data = jQuery(this).attr('href');
				window.location.href = href_data;

            });
        });

        function ShowDialog(modal)
        {
            jQuery("#overlay").show();
            jQuery("#dialog").fadeIn(300);

            if (modal)
            {
                jQuery("#overlay").unbind("click");
            }
            else
            {
                jQuery("#overlay").click(function (e)
                {
                    HideDialog();
                });
            }
        }

        function HideDialog()
        {
            jQuery("#overlay").hide();
            jQuery("#dialog").fadeOut(300);
        }

    </script>
