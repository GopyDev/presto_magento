           context: document.body
			}).done(function(data) {
			alert("information has been updated successfully");
			 // jQuery('#div-'+id).html('<span style="font-weight:bold;color:red;">Status :- Not Found<br/> Note :- '+noteb+'</span>');
		 });
}

