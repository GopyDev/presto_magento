function dohiren333(id)
{
   k=confirm('Are you sure this product is found ? ');
   if(k==true)
   {
       jQuery.ajax({
            url: "https://www.prestofreshgrocery.com/ajax3uu.php?rand="+Math.random()+"&idstring="+id,
           context: document.body
			}).done(function(data) {
			    jQuery('#tr-'+id).hide();
		 });
   }
}
