{
   note= jQuery('#textnote-'+rid.trim()).val();

   jQuery.ajax({
           url: "https://www.prestofreshgrocery.com/ajaxdrivenote.php?rand="+Math.random()+"&note="+note+"&rid="+rid+"&route="+route,
           context: document.body
			}).done(function(data) {
			alert("driver note has been updated successfully");
		 });
}




function dobpack(oid)
{
